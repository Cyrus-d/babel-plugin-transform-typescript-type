{
  "name": "typescript-type-transformer",
  "version": "1.4.9",
  "description": "Transform types to json schema, propTypes and keys.",
  "repository": "https://github.com/Cyrus-d/typescript-type-transformer",
  "license": "MIT",
  "main": "./index.js",
  "types": "./lib/transformerFunctions.d.ts",
  "keywords": [
    "babel-plugin",
    "typescript",
    "interfaces",
    "prop-types",
    "react"
  ],
  "scripts": {
